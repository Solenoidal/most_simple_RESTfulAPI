{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "new-cap": 0,
    "prettier/prettier": [
      1,
      {
        "singleQuote": true,
        "semi": true
      }
    ]
  }
}
